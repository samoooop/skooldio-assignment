{
    "name": "skooldio-assignment",
    "version": "1.0.0",
    "description": "Skooldio backend assignment",
    "main": "index.js",
    "scripts": {
        "build-watch": "tsc -w",
        "build": "tsc",
        "dev-watch": "npm-run-all --parallel build-watch src-watch",
        "dev": "npm-run-all build inspect",
        "inspect": "cross-env TS_NODE_FILES=true NODE_ENV=dev NUM_THREADS=1 node --inspect=0 build/index.js",
        "lint": "eslint -c .eslintrc.js --ext  .ts .",
        "lint-fix": "eslint -c .eslintrc.js --fix --ext .ts .",
        "prod": "node build/index.js",
        "src-watch": "cross-env nodemon --exec node --inspect=0 build/index.js",
        "start": "npm-run-all build prod",
        "test": "jest"
    },
    "author": "Trairat Punyawattananukool",
    "jest": {
        "preset": "ts-jest",
        "rootDir": ".",
        "testRegex": "./test/.*.ts$"
    },
    "license": "MIT",
    "dependencies": {
        "@types/jest": "^24.0.18",
        "@types/node": "^12.7.8",
        "jest": "^24.9.0",
        "moment": "^2.24.0",
        "npm-run-all": "^4.1.5",
        "source-map": "^0.7.3",
        "source-map-support": "^0.5.16",
        "ts-jest": "^24.0.2",
        "ts-node": "^8.4.1",
        "typescript": "^3.7.4",
        "xlsx": "^0.15.4"
    },
    "directories": {
        "test": "test"
    },
    "devDependencies": {
        "@typescript-eslint/eslint-plugin": "^3.6.1",
        "@typescript-eslint/parser": "^3.6.1",
        "eslint": "^7.4.0",
        "eslint-config-prettier": "^6.11.0",
        "eslint-plugin-import": "^2.22.0",
        "eslint-plugin-jsdoc": "^29.2.0",
        "eslint-plugin-no-null": "^1.0.2",
        "eslint-plugin-prefer-arrow": "^1.2.1",
        "eslint-plugin-prettier": "^3.1.4",
        "eslint-plugin-unicorn": "^20.1.0"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/samoooop/skooldio-assignment"
    },
    "homepage": "https://github.com/samoooop/skooldio-assignment",
    "nodemonConfig": {
        "delay": "200"
    }
}
